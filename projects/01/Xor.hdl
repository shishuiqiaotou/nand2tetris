/**
 * Exclusive-or gate:
 * out = (((a == 0) & (b = 1)) | ((a == 1) & (b = 0)), 1, 0)
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a,b=b,out=temp1);
    Nand(a=a,b=temp1,out=temp2);
    Nand(a=temp1,b=b,out=temp3);
    Nand(a=temp2,b=temp3,out=out);
}
